LAMBMONA:LOCAL M%,X,Y,XX,YYDEB::BEEP 50,200DOCLSAT 2,1 :PRINT "MONACO <-> LAMBERT "AT 1,2 :PRINT REPT$("-",20)M%=MENUN(2,"MONACO =---> LAMBERT,LAMBERT =---> MONACO,QUITTER")IF M%=3 :STOPELSEIF M%=1 :GOTO MOLA::ELSEIF M%=2 :GOTO LAMO::ENDIFUNTIL 0MOLA::CLS :BEEP 50,200AT 1,1 :PRINT "MONACO =---> LAMBERT>"AT 1,2 :PRINT REPT$("-",20)AT 1,3 :PRINT "X Mon. ";chr$(63);" "; :input xIF X=0 :GOTO DEB:: :ENDIFAT 1,4 :PRINT "Y Mon. ";chr$(63);" "; :input yXX=(X*.9998233)-(.0000081*Y)+1000004.53YY=(X*-.0000081)+(.9998233*Y)+169998.27CLS :BEEP 50,200AT 1,1 :PRINT "X MONACO =";FIX$(X,3,-10)AT 1,2 :PRINT "Y MONACO =";FIX$(Y,3,-10) AT 1,3 :PRINT "X LAMB.= ";FIX$(XX,2,-10)AT 1,4 :PRINT "Y LAMB.= ";FIX$(YY,2,-10)GETGOTO DEB::LAMO::CLS :BEEP 50,200AT 1,1 :PRINT "LAMBERT =---> MONACO"AT 1,2 :PRINT REPT$("-",20)AT 1,3 :PRINT "X Lamb. ";chr$(63);" "; :input xIF X=0 :GOTO DEB:: :ENDIFAT 1,4 :PRINT "Y Lamb. ";chr$(63);" "; :input yXX=(X*1.00017673)+(.0000081*Y)-1000182.67YY=(X*.0000081)+(1.00017673*Y)-170036.41CLS :BEEP 50,200AT 1,1 :PRINT "X LAMB. =";FIX$(X,2,-10)AT 1,2 :PRINT "Y LAMB. =";FIX$(Y,2,-10)AT 1,3 :PRINT "X MONACO=";FIX$(XX,2,-10)AT 1,4 :PRINT "Y MONACO=";FIX$(YY,2,-10)GETGOTO DEB::