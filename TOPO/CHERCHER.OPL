CHERCHER:
LOCAL NUMENR%,CHERCHE$(30)
DEBUT::
FIRST :CLS :PRINT "CHERCHER:";
TRAP INPUT CHERCHE$
IF ERR=206 :CLOSE :CLS :CARNET:
ENDIF
NUMENR%=FIND(CHERCHE$)
IF NUMENR%=0
PRINT CHR$(16);
CLS :AT 4,3 :PRINT "INTROUVABLE" :PAUSE 20
GOTO DEBUT::
ENDIF
DO
DISP(-1,"") :NEXT :NUMENR%=FIND(CHERCHE$)
IF NUMENR%=0 :CLS
PRINT CHR$(16);
AT 1,3 :PRINT"PAS D";CHR$(39);"AUTRES ENTREES"
GET :CLOSE :CLS :CARNET:
ENDIF
UNTIL 0
