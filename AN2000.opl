AN2000:LOCAL X,Y,H,HH,J,M,MM,S,TX=DAYS(1,1,2000)*86400Y=DAYS(DAY,MONTH,YEAR)*86400H=HOURH=H*3600M=MINUTEM=M*60Y=Y+H+M+SECONDT=X-YJ=INT(T/86400)HH=INT((T-(J*86400))/3600)MM=INT((T-(J*86400)-(HH*3600))/60)S=T-((J*86400)+(HH*3600)+(MM*60))CLSat 6,1 :print "Seulement"PRINT rept$("0",-(j<100))+num$(J,3);" Jours  ";rept$("0",-(hh<10))+num$(hh,2);" Heures";PRINT rept$("0",-(mm<10))+num$(mm,2);" Minutes ";rept$("0",-(s<10))+num$(s,2);" Secon."view(4,"Nous s‚parent de l'AN 2000.")