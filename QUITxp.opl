QUITXP:LOCAL I%,J%,M%,A$(16),CD$(8),XKSTAT 3CLSA$="* AU  REVOIR *"J%=1I%=50WHILE I%<=700BEEP 100,I%AT 1+J%,1 :PRINT MID$(A$,J%,1)AT 1+J%,2 :PRINT "*";I%=I%+50J%=J%+1ENDWHPAUSE 10C:: CLSx=0 :cd$=""OFFAT 1,1 :PRINT "ACCES PROTEGE."AT 1,2 :PRINT "Code:"d:: m%=getif m%<3 or m%=8 :goto c::elseif m%=13 : goto e::endifif x<0 :x=0elseif x=8 :goto c::endifx=x+1AT 5+X,2 :PRINT CHR$(35);" ";CD$=CD$+CHR$(M%)GOTO D::e::if cd$<>"8112" :goto c::endif