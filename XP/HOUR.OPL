TIME:LOCAL A$(30),A1$(3),A2$(3)A:: AT 1,1 :A$=DATIM$A1$=LEFT$(A$,3)IF A1$="MON" :A1$="LUN"ELSEIF A1$="TUE" :A1$="MAR"ELSEIF A1$="WED" :A1$="MER"ELSEIF A1$="THU" :A1$="JEU"ELSEIF A1$="FRI" :A1$="VEN"ELSEIF A1$="SAT" :A1$="SAM"ELSEIF A1$="SUN" :A1$="DIM"ENDIFA2$=MID$(A$,8,3)IF A2$="FEB" :A2$="FEV"ELSEIF A2$="APR" :A2$="AVR"ELSEIF A2$="MAY" :A2$="MAI"ELSEIF A2$="JUL" :A2$="JUI"ELSEIF A2$="AUG" :A2$="AOU"ENDIFA$=A1$+MID$(A$,4,4)+A2$+RIGHT$(A$,14)A$=LEFT$(A$,15)+"     "+RIGHT$(A$,8)PRINT A$GOTO A::