FORMELEC:LOCAL M%,m2%,V,A,O,W,CV,R1,R2,R3,R4,R5,RTOT,RC,RD,RESRECA:: CV=0 :V=0 :A=0 :O=0 :W=0 :CLS :M%=MENU("Watts,Volts,Amperes,Ohms,Resistances") :clsif m%=1 :goto watt::elseif m%=2 :goto volt::elseif m%=3 :goto amp::elseif m%=4 :GOTO ohm::ELSEIF M%=5 :GOTO RESI::endifSTOPwatt:: print"Volts :"; :input v :print"Amperes :"; :input a :print"Ohms :"; :input oIF O=0 :W=V*AELSEIF A=0 :W=(V*V)/OELSEIF V=0 :W=(A*A)*OENDIFcls :at 1,1 : print"WATTS : ";W :CV=W/736 :CV=INT(CV*100) :CV=CV/100 :AT 1,2 :PRINT"CV : ";CV :GET :GOTO A::volt:: PRINT"WATTS : "; :INPUT W :PRINT "AMPERES : "; :INPUT A :PRINT "OHMS : "; :INPUT OIF W=0 :V=O*AELSEIF A=0 :V=SQR(W*O)ELSEIF O=0 :V=W/AENDIFCLS :AT 1,1 :PRINT"VOLTS : ";INT(100*V)/100 :GET :GOTO A::amp:: PRINT"VOLTS : "; :INPUT V :PRINT"WATTS : "; :INPUT W :PRINT"OHMS : "; :INPUT OIF V=0 :A=SQR(W/O)ELSEIF W=0 :A=V/OELSEIF O=0 :A=W/VENDIFCLS :AT 1,1 :A=INT(A*100) :A=A/100 :PRINT "AMPERES : ";A :GET :GOTO A::ohm:: PRINT"VOLTS : "; :INPUT V :PRINT"AMPERES : "; :INPUT A :PRINT"WATTS : "; :INPUT WIF V=0 :O=W/(A*A)ELSEIF A=0 :O=(V*V)/WELSEIF W=0 :O=V/AENDIFCLS :AT 1,2 :PRINT"OHMS : ";INT(O*100)/100 :GET :GOTO A::RESI:: R1=0 :R2=0 :R3=0 :R4=0 :R5=0 :RTOT=0 :RC=0 :RD=0 :RESREC=0 :M2%=MENU("Parallele,Rajout-resi") :CLSif m2%=1 :goto para::elseif m2%=2 :goto ajou::endifgoto a::para:: PRINT"5 maxi (0=stop)" :print"VALEUR No1: "; :input r1if r1=0 :goto para::endifr1=1/r1 :print "VALEUR No2: "; :input r2if r2=0 :goto para::endifr2=1/r2 :print"VALEUR No3: "; :input r3if r3=0 :goto tot::endifr3=1/r3 :print"VALEUR No4: "; :INPUT R4IF R4=0 :GOTO TOT::ENDIFR4=1/R4 :PRINT"VALEUR No5: "; :input r5if r5=0 :goto para::endifr5=1/r5tot:: rtot=1/(r1+r2+r3+r4+r5) :RTOT=INT(RTOT*100) :RTOT=RTOT/100 :cls :at 1,1 :print"RESIS. TOTALE" :PRINT RTOT :GET :GOTO RESI::AJOU:: PRINT "RESIS. CONNUE :" :INPUT RC :CLS :PRINT"RESIS. DESIREE" :INPUT RDIF RD>=RC :PRINT "ERREUR..." :PAUSE 50 :GOTO RESI::ENDIFRESREC=1/(1/RC-1/RD) :RESREC=RESREC-RESREC*2 :RESREC=INT(100*RESREC) :RESREC=RESREC/100 :CLS :PRINT"RESI. RECHERCHEE"; :PRINT RESREC :GET :GOTO RESI::