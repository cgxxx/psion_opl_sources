TAXES:local t,ht,ttc,m%a:: ttc=0 :ht=0 :t=0m%=menu("TTC,HT,TAUX,QUITTER")if m%=1 :goto ttc::elseif m%=2 :goto ht::elseif m%=3 :goto t::ELSEIF M%=4 :RETURNelse RETURNendifttc:: print "Prix.HT :"; :INPUT htPRINT "Taxe en % :"; :input tTTC=(ht*T/100)cls :print "   Prix TTC"PRINT fix$((ht+TTC),2,-10);" F"GET :GOTO A::ht:: print "Prix.TTC" :INPUT TTCPRINT "Taxe en % :"; :input tht=ttc/(t/100+1)cls :print "   Prix HT"PRINT FIX$(HT,2,-10);" F"GET :GOTO A::t::PRINT "Prix.TTC :"; :INPUT TTCPRINT "Prix.HT  :"; :INPUT HTT=(TTC/(HT/100)-100)CLS :PRINT"Taux de la taxe:";AT 5,2 :print FIX$(t,2,-5);" %"get :goto a::