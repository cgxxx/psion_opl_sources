FUSEAUX:LOCAL H,M,S,D,A$(20),I,MNM=1A:: H=HOURMN=MINUTES=SECONDIF M=1 :A$="PARIS" :D=0ELSEIF M=2 :A$="MOSCOU" :D=2ELSEIF M=3 :A$="TOKYO" :D=9ELSEIF M=4 :A$="SYDNEY" :D=9ELSEIF M=5 :A$="BAGDAD" :D=2ELSEIF M=6 :A$="DAKAR" :D=-2ELSEIF M=7 :A$="BRASILIA" :D=-4ELSEIF M=8 :A$="NEW YORK" :D=-6ELSEIF M=9 :A$="LOS ANGELES" :D=-9ELSEIF M=10 :A$="QUEBEC" :D=-6ELSEIF M=11 :STOPENDIFH=H+DIF H<0 :H=H+24ELSEIF H>23 :H=H-24ENDIFAT 1,1 :PRINT A$ :AT 14,1 :PRINT DAT 5,2 :PRINT H;":";MN;":";S;"  ";I=I+1IF I=100 :M=M+1 :CLS :I=0ENDIFGOTO A::